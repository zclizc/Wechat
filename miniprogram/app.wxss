body {
  margin: 0;
  padding: 0;
  font-family: "Helvetica Neue", "Arial", sans-serif;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100vh;
}

.button {
  margin: 10px;
